{
    "name": "unikent/programmes-plant-api-php",
    "version": "0.1",
    "type": "library",
    "description": "A library for interacting with the Programmes Plant REST API.",
    "homepage": "http://github.com/unikent/programmes-plant-api-php",
    "license": "GPL-3.0+",
    "require": {
        "laravel/curl": "dev-master",
        "guzzle/guzzle": "3.1.2"
    },
    "require-dev": {
        "mockery/mockery": ">=0.7.2",
        "phpunit/phpunit": "*"
    },
    "authors": [
        {
            "name": "Alex Andrews",
            "email": "a.andrews@kent.ac.uk"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-0": { "ProgrammesPlant\\": "src/" }
    },
    "minimum-stability": "dev"
}
